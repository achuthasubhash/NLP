   "source": [
    "from textblob import TextBlob\n",
    "\n",
    "def spell_corrector(sentence):\n",
    "    tb = TextBlob(sentence)\n",
    "    correct = tb.correct()\n",
    "    print(\"Orignal Sentence: \",sentence)\n",
    "    print(\"Corrected sentence: \",correct)\n",
    "    return correct\n",
    "sen = 'plese corret sentene'\n",
    "spell_corrector(sen)"
   ]
